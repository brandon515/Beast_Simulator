./makeCreate.py release "-Wall -c -std=gnu++11"  "-Wall -L/usr/local/lib -Wl,-rpath,/usr/local/lib -lSDL2 -lSDL2_image -lboost_system -lboost_chrono -ljson -L/usr/lib -lpthread -ldl  -lutil -lm  -lpython2.7 -Xlinker -export-dynamic -Wl,-O1 -Wl,-Bsymbolic-functions" ./exe/Named
./makeCreate.py debug "-Wall -c -g -Werror -std=gnu++11"  "-Wall -L/usr/local/lib -Wl,-rpath,/usr/local/lib -lSDL2 -lSDL2_image -lboost_system -lboost_chrono -ljson -L/usr/lib -lpthread -ldl  -lutil -lm  -lpython2.7 -Xlinker -export-dynamic -Wl,-O1 -Wl,-Bsymbolic-functions" ./exe/Named_debug debug
